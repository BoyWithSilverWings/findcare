{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1,
    "no-plusplus": 0
  },
  "globals": {
    
  }
}